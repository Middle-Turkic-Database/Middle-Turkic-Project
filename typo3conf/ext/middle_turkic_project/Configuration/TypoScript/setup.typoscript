page {
    includeCSS {
        fonts = EXT:middle_turkic_project/Resources/Public/Css/fonts.css
        mdb = EXT:middle_turkic_project/Resources/Public/Css/mdb-free.min.css
        mdb_datatables = EXT:middle_turkic_project/Resources/Public/Css/datatables.css
        theme = EXT:middle_turkic_project/Resources/Public/Scss/Theme/theme.scss
        toastr = https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css
        toastr.external = 1
        dot-pulse = EXT:middle_turkic_project/Resources/Public/Css/dot-pulse.css
    }

    includeJSLibs {

    }

    includeJS {

    }

    includeJSFooterlibs {
    }

    includeJSFooter {
        middle_turkic_project_scripts = EXT:middle_turkic_project/Resources/Public/JavaScript/Dist/scripts.js
        manuscript_list = EXT:middle_turkic_project/Resources/Public/JavaScript/Dist/manuscriptList.js
        mdb = EXT:middle_turkic_project/Resources/Public/JavaScript/Dist/mdb.min.js
        toastr = https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js
        toastr.external = 1
